				<LinearLayout
					android:id="@+id/linear19"
					android:layout_width="match_parent"
					android:layout_height="60dp"
					android:layout_marginBottom="10dp"
					android:orientation="horizontal">
					<Button
						android:id="@+id/button11"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
